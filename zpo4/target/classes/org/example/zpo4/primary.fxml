<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="382.0" prefWidth="500.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.zpo4.PrimaryController">
   <children>
      <TextField fx:id="inputField" />
      <Button fx:id="classBtn" onAction="#classAction" text="Get fields from Class" />
      <TableView fx:id="objectTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="objectCol" prefWidth="459.0" text="Objects" />
        </columns>
      </TableView>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="fieldsCol" prefWidth="459.0" text="Fields" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
